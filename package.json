{
	"name": "@jdeighan/cielo",
	"version": "8.1.73",
	"type": "module",
	"preferGlobal": true,
	"bin": {
		"cielo": "./bin/cielo.js"
	},
	"description": "command to process *.cielo and *.starbucks files",
	"main": "./bin/cielo.js",
	"scripts": {
		"build": "coffee --no-header -c .",
		"pretest": "cls && coffee --bare --no-header -c .",
		"test": "ava ./test/*.test.js",
		"prefinaltest": "cls && coffee --bare --no-header -c .",
		"finaltest": "cross-env FINALTEST=yes ava ./test/*.test.js",
		"precielo": "coffee --no-header -c .",
		"cielo": "node ./bin/cielo.js"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"coffeescript"
	],
	"author": "John Deighan",
	"license": "MIT",
	"dependencies": {
		"@jdeighan/coffee-utils": "^4.1.23",
		"@jdeighan/env": "^8.0.36",
		"@jdeighan/starbucks": "^7.0.38",
		"@jdeighan/string-input": "^8.0.47",
		"ava": "^3.15.0",
		"chokidar": "^3.5.3",
		"cross-env": "^7.0.3",
		"minimist": "^1.2.5",
		"svelte": "^3.46.2"
	}
}
