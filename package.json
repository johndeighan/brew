{
	"name": "@jdeighan/brew",
	"version": "3.0.1",
	"type": "module",
	"preferGlobal": true,
	"bin": {
		"cielo": "./bin/brew.js"
	},
	"description": "command to process Starbucks files",
	"main": "./bin/brew.js",
	"scripts": {
		"build": "coffee --bare --no-header -c .",
		"pretest": "coffee --bare --no-header -c . && node ./bin/brew.js ./test",
		"test": "ava ./test/*.test.js",
		"prefinaltest": "cls && coffee --bare --no-header -c . && node ./bin/brew.js ./test",
		"finaltest": "cross-env FINALTEST=yes ava ./test/*.test.js"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"coffeescript"
	],
	"author": "John Deighan",
	"license": "MIT",
	"dependencies": {
		"@jdeighan/coffee-utils": "^3.0.4",
		"@jdeighan/env": "^4.0.2",
		"@jdeighan/starbucks": "^4.0.3",
		"cross-env": "^7.0.3",
		"minimist": "^1.2.5"
	}
}
