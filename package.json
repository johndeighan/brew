{
	"name": "@jdeighan/cielo",
	"version": "4.0.0",
	"type": "module",
	"preferGlobal": true,
	"bin": {
		"cielo": "./bin/cielo.js"
	},
	"description": "command to process *.cielo and *.starbucks files",
	"main": "./bin/cielo.js",
	"scripts": {
		"build": "coffee --bare --no-header -c .",
		"pretest": "cls && coffee --bare --no-header -c .",
		"test": "ava ./test/*.test.js",
		"prefinaltest": "cls && coffee --bare --no-header -c .",
		"finaltest": "cross-env FINALTEST=yes ava ./test/*.test.js",
		"precielo": "coffee --bare --no-header -c .",
		"cielo": "node ./bin/cielo.js"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"coffeescript"
	],
	"author": "John Deighan",
	"license": "MIT",
	"dependencies": {
		"@jdeighan/coffee-utils": "^3.0.4",
		"@jdeighan/env": "^4.0.4",
		"@jdeighan/starbucks": "^4.0.5",
		"cross-env": "^7.0.3",
		"minimist": "^1.2.5"
	}
}
